{
  "name": "solid-blog",
  "version": "0.0.2",
  "description": "SolidJS + Stiches + Vanilla Extract + TailwindCSS 3.0",
  "scripts": {
    "build": "pnpm build:css && vite build",
    "build:css": "postcss src/tailwind.scss -o src/main.css --env production",
    "dev": "concurrently \"vite\" \"pnpm dev:css\"",
    "dev:css": "postcss src/tailwind.scss -o src/main.css --watch",
    "serve": "vite preview",
    "start": "vite"
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.5.1",
    "pnpm": "^6.24.3",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "postcss-nesting": "^10.1.0",
    "tailwindcss": "^3.0.7",
    "typescript": "^4.5.4",
    "vite": "^2.7.6",
    "vite-plugin-solid": "^2.1.4"
  },
  "dependencies": {
    "@stitches/core": "^1.2.6",
    "@vanilla-extract/css": "^1.6.8",
    "@vanilla-extract/css-utils": "^0.1.2",
    "@vanilla-extract/dynamic": "^2.0.2",
    "@vanilla-extract/recipes": "^0.2.3",
    "@vanilla-extract/sprinkles": "^1.3.3",
    "@vanilla-extract/vite-plugin": "^3.1.2",
    "solid-js": "^1.2.6"
  }
}
